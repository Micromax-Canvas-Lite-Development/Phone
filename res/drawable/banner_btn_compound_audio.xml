<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/compoundBackgroundItem" android:drawable="@drawable/banner_btn_compound_background" />
    <item android:id="@id/bluetoothItem">
        <bitmap android:gravity="center" android:src="@drawable/ic_sound_bluetooth_holo_dark" />
    </item>
    <item android:id="@id/handsetItem">
        <bitmap android:gravity="center" android:src="@drawable/ic_sound_handset_holo_dark" />
    </item>
    <item android:id="@id/speakerphoneOnItem">
        <bitmap android:gravity="center" android:src="@drawable/banner_icon_speaker" />
    </item>
    <item android:id="@id/bluetooth_more">
        <bitmap android:gravity="center" android:src="@drawable/banner_icon_bluetooth_with_menu" />
    </item>
    <item android:id="@id/speaker_more">
        <bitmap android:gravity="center" android:src="@drawable/banner_icon_speaker_with_menu" />
    </item>
    <item android:id="@id/handset_more">
        <bitmap android:gravity="center" android:src="@drawable/banner_icon_receiver_with_menu" />
    </item>
</layer-list>